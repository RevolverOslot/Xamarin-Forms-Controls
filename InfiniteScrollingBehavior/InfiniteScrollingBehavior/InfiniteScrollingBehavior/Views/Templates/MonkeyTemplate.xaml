<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="InfiniteScrollingBehavior.Views.Templates.MonkeyTemplate">
  <ContentView.Content>
        <Grid>
            <Label 
                Text="{Binding Name}"
                VerticalOptions="CenterAndExpand"/>
        </Grid>
  </ContentView.Content>
</ContentView>
